{
    "name": "solid-data-workers/laravel-sparql",
    "description": "A SPARQL based Eloquent model and Query builder for Laravel",
    "keywords": ["laravel","eloquent","sparql","database","model","rdf"],
    "homepage": "https://gitlab.com/solid-data-workers/laravel-sparql",
    "authors": [
        {
            "name": "Roberto Guido",
            "homepage": "https://madbob.org",
            "email": "bob@linux.it"
        }
    ],
    "license" : "MIT",
    "require": {
        "illuminate/support": "^5.8",
        "illuminate/container": "^5.8",
        "illuminate/database": "^5.8",
        "illuminate/events": "^5.8",
        "madbob/easyrdf-on-guzzle": "^0.1"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "psr-0": {
            "SolidDataWorkers\\SPARQL": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolidDataWorkers\\SPARQL\\SPARQLEndpointServiceProvider"
            ]
        }
    }
}
